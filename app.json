{
  "expo": {
    "name": "Flashcard App",
    "slug": "flashcard-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "flashcardapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thanhdanhdo1701.flashcardapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thanhdanhdo1701.flashcardapp",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "The application needs access to the image library to scan the text.",
          "cameraPermission": "The application needs access to the camera to take photos and scan the text."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "70cd2f55-fc1b-4049-b43e-762abb2103e9"
      }
    },
    "owner": "thanhdanhdo1701"
  }
}
